
public class Student {
	String name;

	Student(String name) {
		this.name = name;
	}
	int Play(int game){
		
		if(game == 1){
			System.out.println(name + " is playing B-ball.");
		}
		else if(game == 1){
			System.out.println(name + " is playing Soccer.");	
		}
		else{
			System.out.println(name + " is laying unknown game.");
		}
		
		return 10;
	}
}
